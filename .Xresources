!! Xft
Xft.dpi: 96
Xft.antialias: true

!! Appearance
urxvt.termName: rxvt-unicode
urxvt.scrollBar: false

!! Font
URxvt*font: xft:Source Code Pro:size=11:hinting=true
URxvt*boldFont: xft:Source Code Pro:size=11:hinting=true
URxvt*italicFont: xft:Source Code Pro:style=Oblique:size=11:hinting=true
URxvt*boldItalicFont: xft:Source Code Pro:style=Bold Oblique:size=11:hinting=true
URxvt*letterSpace: 0

!! Color
urxvt.background: #242424
urxvt.foreground: #f2f2f2

! Black + DarkGrey
*color0:  #000000
*color8:  #555753
! DarkRed + Red
*color1:  #ff6565
*color9:  #ff8d8d
! DarkGreen + Green
*color2:  #93d44f
*color10: #c8e7a8
! DarkYellow + Yellow
*color3:  #eab93d
*color11: #ffc123
! DarkBlue + Blue
*color4:  #204a87
*color12: #3465a4
! DarkMagenta + Magenta
*color5:  #ce5c00
*color13: #f57900
!DarkCyan + Cyan (both not tango)
*color6:  #89b6e2
*color14: #46a4ff
! LightGrey + White
*color7:  #cccccc
*color15: #ffffff